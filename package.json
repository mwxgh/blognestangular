{
  "name": "blog",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "start:dev": "nx run-many --parallel --target=serve --projects=api,main",
    "storybook": "nx run shared-components:storybook",
    "generate:storybook:module": "nx g @nrwl/angular:module --project=shared-components",
    "generate:storybook:component": "nx g @nrwl/angular:component --project=shared-components",
    "generate:storybook": "nx g @nrwl/angular:stories shared-components",
    "g:main:c": "nx g @nrwl/angular:component --project=main",
    "g:main:m": "nx g @nrwl/angular:module --project=main",
    "g:main:s": "nx g @nrwl/angular:service --project=main",
    "g:main:d": "nx g @nrwl/angular:directive --project=main",
    "g:api:s": "nx g @nrwl/nest:service --project=api",
    "g:api:m": "nx g @nrwl/nest:module --project=api",
    "g:api:c": "nx g @nrwl/nest:controller --project=api",
    "build": "nx run-many --parallel --target=production --projects=api,main",
    "test": "nx test",
    "postinstall": "ngcc --properties es2015 browser module main",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./apps/api/config/ormconfig.ts",
    "migration:create": "npm run typeorm migration:create -- -d ./apps/api/database/migrations -n",
    "db:migrate": "npm run typeorm migration:run",
    "db:seed": "npm run command seed --",
    "angular:g:module": "nx g module --project=main",
    "command": "node --require ts-node/register command.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@nestjs/axios": "^0.0.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-fastify": "^8.2.4",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.2.4",
    "@nrwl/angular": "13.4.3",
    "app-master": "^1.0.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "excel4node": "^1.7.2",
    "fastify-multipart": "^5.2.1",
    "fastify-static": "^4.5.0",
    "fastify-swagger": "^4.13.0",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "ngx-admin": "^0.0.0-beta.0",
    "ngx-toastr": "^14.2.1",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "shelljs": "^0.8.4",
    "slugify": "^1.6.5",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.41",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.4.3",
    "@nrwl/cypress": "13.4.3",
    "@nrwl/eslint-plugin-nx": "13.4.3",
    "@nrwl/jest": "13.4.3",
    "@nrwl/linter": "13.4.3",
    "@nrwl/nest": "13.4.3",
    "@nrwl/node": "13.4.3",
    "@nrwl/tao": "13.4.3",
    "@nrwl/workspace": "13.4.3",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "cypress": "^9.1.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  }
}
